pipeline{
     agent any
     environment{
               PATH="/opt/maven/bin:$PATH"
    }
     stages{

        stage("Git Checkout"){

           steps{

               git credentialsId: 'github', url: 'https://github.com/joepapson/mypipelineweb'
       }   
     }
        stage("Maven Build"){
               steps{
                      sh "mvn clean package"
              }
 }
   }
}
